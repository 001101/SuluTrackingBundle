{
    "name": "sulu/tracking-bundle",
    "type": "sulu-bundle",
    "description": "The Sulu Bundle which provides tracking functionality",
    "license": "MIT",
    "keywords": [
        "sulu",
        "tracking"
    ],
    "authors": [
        {
            "name": "Sulu Tracking",
            "homepage": "https://github.com/sulu/SuluTrackingBundle/contributors"
        }
    ],
    "require": {
        "php": "~5.6 || ~7.0",
        "symfony/framework-bundle": "~2.8 || ~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0 || ~6.0",
        "symfony/monolog-bundle": "~2.8 || ~3.0"
    },
    "autoload": {
        "psr-4": {
            "Sulu\\Bundle\\TrackingBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sulu\\Bundle\\TrackingBundle\\Tests\\": "tests/"
        }
    }
}
